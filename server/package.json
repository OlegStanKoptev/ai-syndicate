{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "db:reset": "rm -rf prisma/migrations & prisma migrate dev --name initial",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "lint": "eslint --max-warnings=0 --cache --cache-location ./node_modules/.cache/eslint --ignore-path .gitignore .",
    "ts:check": "tsc -b",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx)\" --ignore-path .gitignore",
    "format:check": "prettier --list-different \"**/*.+(js|jsx|ts|tsx)\" --ignore-path .gitignore",
    "validate": "concurrently \"npm run format:check\" \"npm run lint\" \"npm run ts:check\""
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "eslintConfig": {
    "extends": [
      "@remix-run/eslint-config",
      "@remix-run/eslint-config/node",
      "prettier"
    ]
  },
  "prettier": {},
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@prisma/client": "^4.11.0",
    "@remix-run/node": "1.13.0",
    "@remix-run/react": "1.13.0",
    "@remix-run/serve": "1.13.0",
    "@types/node-schedule": "^2.1.0",
    "@types/react-datepicker": "^4.10.0",
    "bcryptjs": "^2.4.3",
    "eslint-config-prettier": "^8.7.0",
    "immer": "^9.0.19",
    "isbot": "^3.6.5",
    "node-schedule": "^2.1.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-icons": "^4.8.0",
    "react-select": "^5.7.0",
    "spin-delay": "^1.2.0",
    "superjson": "^1.12.2",
    "tailwind-merge": "^1.10.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "1.13.0",
    "@remix-run/eslint-config": "1.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.6.0",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.35.0",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
